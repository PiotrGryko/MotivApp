<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_height="30dp"
    android:layout_width="match_parent"
    android:id="@+id/constraintlayout00">
    <android.widget.ImageView
        android:layout_height="0dip"
        android:layout_width="0dip"
        android:scaleType="fitXY"
        android:id="@+id/field0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="@id/guidelineVertical010"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent">
    </android.widget.ImageView>
    <android.widget.TextView
        android:layout_height="0dip"
        android:layout_width="0dip"
        android:id="@+id/field1"
        app:layout_constraintLeft_toLeftOf="@id/guidelineVertical013"
        app:layout_constraintRight_toRightOf="@id/guidelineVertical113"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent">
    </android.widget.TextView>
    <androidx.constraintlayout.widget.Guideline
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical"
        android:id="@+id/guidelineVertical010"
        app:layout_constraintGuide_percent="0.10">
    </androidx.constraintlayout.widget.Guideline>
    <androidx.constraintlayout.widget.Guideline
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical"
        android:id="@+id/guidelineVertical113"
        app:layout_constraintGuide_percent="1.13">
    </androidx.constraintlayout.widget.Guideline>
    <androidx.constraintlayout.widget.Guideline
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical"
        android:id="@+id/guidelineVertical013"
        app:layout_constraintGuide_percent="0.13">
    </androidx.constraintlayout.widget.Guideline>
</androidx.constraintlayout.widget.ConstraintLayout>